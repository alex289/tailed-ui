{
  "compilerOptions": {
    "jsx": "react",
    "module": "ESNext",
    "moduleResolution": "node",
    "noImplicitAny": true,
    "removeComments": true,
    "sourceMap": true,
    "target": "es5",
    "esModuleInterop":true,
    "strict": true,
    "incremental": true,
    "skipLibCheck": true,
    "outDir": "dist",
  },
  "include": [
    "src/**/*"
  ],
  "exclude": ["src/**/*.stories.tsx"]
}
